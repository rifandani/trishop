{
  "name": "trishop",
  "description": "Fullstack e-commerce website built with Next.js",
  "version": "0.3.1",
  "private": false,
  "author": {
    "name": "Tri Rizeki Rifandani",
    "email": "tri.rifandani@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "reactjs",
    "nextjs",
    "tailwindcss",
    "swr",
    "axios",
    "gridjs"
  ],
  "scripts": {
    "dev": "rm -rf ./.next && next dev",
    "build": "next build",
    "start": "next start",
    "build:export": "next build && next export",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cy:install": "cypress install",
    "cy:open": "cypress open --browser chrome",
    "cy:run": "cypress run --headless --browser chrome",
    "cm": "cz",
    "release": "standard-version",
    "release:first": "standard-version --first-release",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@cloudinary/react": "^1.4.0",
    "@cloudinary/url-gen": "^1.8.0",
    "@headlessui/react": "^1.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.30.0",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "formik": "^2.2.9",
    "gridjs": "^5.0.2",
    "gridjs-react": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "midtrans-client": "^1.2.4",
    "mongoose": "^5.12.14",
    "next": "^12.1.6",
    "next-connect": "^0.10.1",
    "next-seo": "^5.4.0",
    "nodemailer": "^6.6.2",
    "nprogress": "^0.2.0",
    "react": "^18.1.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.1",
    "react-icons": "^4.3.1",
    "react-paginate": "^8.1.3",
    "react-redux": "^8.0.1",
    "react-toastify": "^9.0.1",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^3.0.1",
    "swiper": "^8.1.5",
    "swr": "^1.3.0",
    "yup": "^0.32.11",
    "yup-phone": "^1.3.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.3.0",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.0",
    "@tailwindcss/forms": "^0.5.1",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.10",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.34",
    "@types/nodemailer": "^6.4.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.9",
    "@types/react-datepicker": "^4.4.1",
    "@types/react-dom": "^18.0.4",
    "@types/react-paginate": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "autoprefixer": "^10.4.7",
    "chromatic": "^5.9.2",
    "commitizen": "^4.2.4",
    "cypress": "^7.6.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "standard-version": "^9.3.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.0.0",
    "typescript": "^4.6.4"
  }
}
