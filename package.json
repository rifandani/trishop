{
  "name": "trishop",
  "description": "Fullstack e-commerce website built with Next.js",
  "version": "0.2.0",
  "private": false,
  "author": {
    "name": "Tri Rizeki Rifandani",
    "email": "tri.rifandani@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "reactjs",
    "nextjs",
    "tailwindcss",
    "swr",
    "axios",
    "gridjs"
  ],
  "scripts": {
    "dev": "rm -rf ./.next && next dev",
    "build": "next build",
    "start": "next start",
    "build:export": "next build && next export",
    "prepare": "husky install",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@cloudinary/base": "^1.0.0-beta.3",
    "@cloudinary/react": "^1.0.0-beta.8",
    "@headlessui/react": "^1.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/line-clamp": "^0.2.1",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.26.0",
    "cookie": "^0.4.1",
    "dayjs": "^1.10.5",
    "formik": "^2.2.8",
    "gridjs": "^4.0.0",
    "gridjs-react": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "midtrans-client": "^1.2.3",
    "mongoose": "^5.12.10",
    "next": "10.2.2",
    "next-seo": "^4.24.0",
    "nodemailer": "^6.6.0",
    "nprogress": "^0.2.0",
    "postcss": "^8.3.0",
    "react": "17.0.2",
    "react-datepicker": "^4.1.1",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.3.2",
    "react-icons": "^4.2.0",
    "react-paginate": "^7.1.3",
    "react-toastify": "^7.0.4",
    "swiper": "^6.6.2",
    "swr": "^0.5.6",
    "tailwindcss": "^2.1.2",
    "yup": "^0.32.9",
    "yup-phone": "^1.2.19"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.4.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^15.6.0",
    "@types/nodemailer": "^6.4.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.6",
    "@types/react-datepicker": "^3.1.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-paginate": "^7.1.0",
    "@types/react-toastify": "^4.1.0",
    "@types/swiper": "^5.4.2",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
