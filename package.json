{
  "name": "trishop",
  "description": "Fullstack e-commerce website built with Next.js",
  "version": "0.2.0",
  "private": false,
  "author": {
    "name": "Tri Rizeki Rifandani",
    "email": "tri.rifandani@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "reactjs",
    "nextjs",
    "tailwindcss",
    "swr",
    "axios",
    "gridjs"
  ],
  "scripts": {
    "dev": "rm -rf ./.next && next dev",
    "build": "next build",
    "start": "next start",
    "build:export": "next build && next export",
    "prepare": "husky install",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cm": "cz",
    "cm:signed": "cz -S",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@cloudinary/base": "^1.0.0-beta.3",
    "@cloudinary/react": "^1.0.0-beta.8",
    "@headlessui/react": "^1.2.0",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.1",
    "autoprefixer": "^10.2.6",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.26.0",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "dayjs": "^1.10.5",
    "formik": "^2.2.9",
    "gridjs": "^5.0.2",
    "gridjs-react": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "midtrans-client": "^1.2.4",
    "mongoose": "^5.12.14",
    "next": "11.0.0",
    "next-seo": "^4.25.1",
    "nodemailer": "^6.6.1",
    "nprogress": "^0.2.0",
    "postcss": "^8.3.5",
    "react": "17.0.2",
    "react-datepicker": "^4.1.1",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.3.2",
    "react-icons": "^4.2.0",
    "react-paginate": "^7.1.3",
    "react-toastify": "^7.0.4",
    "swiper": "^6.7.0",
    "swr": "^0.5.6",
    "tailwindcss": "^2.1.4",
    "yup": "^0.32.9",
    "yup-phone": "^1.2.19"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.2.9",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.10",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.12.2",
    "@types/nodemailer": "^6.4.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.11",
    "@types/react-datepicker": "^3.1.8",
    "@types/react-dom": "^17.0.7",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-paginate": "^7.1.0",
    "@types/react-toastify": "^4.1.0",
    "@types/swiper": "^5.4.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.9.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.3.1",
    "standard-version": "^9.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
